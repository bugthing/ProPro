<div class="accordion-group section-line-edit" id="section-line-<%= section_line.id %>">
  <div class="accordion-heading">
    <%= link_to(image_tag('/icons/delete.png'), section_line, :method => :delete, :remote => true, :class => "pull-right", :"data-section-line-id" => section_line.id) %>
    <a class="accordion-toggle" data-toggle="collapse" data-parent="#section-builder-lines" href="#sectionLineCollapse<%= section_line.id %>"> 
      Section Line <%= section_line.id %>
    </a>
  </div>

  <div id="sectionLineCollapse<%= section_line.id %>" class="accordion-body collapse">
    <div class="accordion-inner"> 

<%= form_for(section_line, :remote => true, :html => { :class => "section-line-edit"}) do |f| %>
  <%= f.hidden_field :weight %>
  <%= raw section_line.propro_tool.edit_html %>
<% end %>

    </div>
  </div>
</div>

